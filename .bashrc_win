if [[ -f /usr/share/vim/vim74-kaoriya-win64/gvim.exe ]]; then
    export EDITOR=/usr/share/vim/vim74-kaoriya-win64/gvim.exe
    alias gvim='function _gvim() if [ $# -eq 0 ]; then $EDITOR "$@"; else $EDITOR --remote-tab-silent "$@"; fi; _gvim "$@"'
#    alias vim=gvim
#    alias vi=vim
fi

alias javac='javac -J-Dfile.encoding=UTF-8'
alias java='java -Dfile.encoding=UTF-8'
alias jar='jar -J-Dfile.encoding=UTF-8'

export GRADLE_OPTS=-Dfile.encoding=UTF-8

