#!/bin/bash

s1="\033[32m"
s2="\033[33m"
s3="\033[7;31m"
s4="\033[7;36m"
s5="\033[7;32m"
s6="\033[7;33m"
s0="\033[7;35m"
e="\033[0m"

echo -e "
-----------------------------------------------------------------------------
 [${s1}ESC${e}][${s1} 1 ${e}][${s1} 2 ${e}][${s1} 3 ${e}][${s1} 4 ${e}][${s1} 5 ${e}][${s2} 6 ${e}][${s2} 7 ${e}][${s2} 8 ${e}][${s2} 9 ${e}][${s2} 0 ${e}][${s4} - ${e}][${s4} ^ ${e}][${s0} ] ${e}][${s4} \\ ${e}]
 [${s1} TAB ${e}][${s1} q ${e}][${s1} w ${e}][${s1} e ${e}][${s1} r ${e}][${s1} t ${e}][${s2} y ${e}][${s2} u ${e}][${s2} i ${e}][${s2} o ${e}][${s2} p ${e}][${s4} @ ${e}][${s4} [ ${e}][${s2}   BS ${e}]
 [${s1} CTRL  ${e}][${s1} a ${e}][${s1} s ${e}][${s1} d ${e}][${s5} f ${e}][${s1} g ${e}][${s2} h ${e}][${s6} j ${e}][${s2} k ${e}][${s2} l ${e}][${s4} ; ${e}][${s4} : ${e}][${s2}   ENTER ${e}]
 [${s1} SHIFT  ${e}][${s1} z ${e}][${s1} x ${e}][${s1} c ${e}][${s1} v ${e}][${s1} b ${e}][${s2} n ${e}][${s2} m ${e}][${s4} , ${e}][${s4} . ${e}][${s4} / ${e}][${s2}   SHIFT ${e}][${s2}Fn${e}]
        [${s1}Fn ${e}][${s1} ALT ${e}][${s1}           SPACE           ${e}][${s2} ALT ${e}][${s4}WIN${e}]
             [${s4} Han ${e}]                             [${s4} Zen ${e}]

-----------------------------------------------------------------------------
 [${s1}ESC${e}][${s4} ! ${e}][${s4} \" ${e}][${s4} # ${e}][${s4} $ ${e}][${s4} % ${e}][${s4} & ${e}][${s4} ' ${e}][${s4} ( ${e}][${s4} ) ${e}][${s2}   ${e}][${s4} = ${e}][${s4} ~ ${e}][${s0} } ${e}][${s4} | ${e}]
 [${s1} TAB ${e}][${s1} Q ${e}][${s1} W ${e}][${s1} E ${e}][${s1} R ${e}][${s1} T ${e}][${s2} Y ${e}][${s2} U ${e}][${s2} I ${e}][${s2} O ${e}][${s2} P ${e}][${s4} \` ${e}][${s4} { ${e}][${s2}   BS ${e}]
 [${s1} CTRL  ${e}][${s1} A ${e}][${s1} S ${e}][${s1} D ${e}][${s5} F ${e}][${s1} G ${e}][${s2} H ${e}][${s6} J ${e}][${s2} K ${e}][${s2} I ${e}][${s4} + ${e}][${s4} * ${e}][${s2}   ENTER ${e}]
 [${s3} SHIFT  ${e}][${s1} Z ${e}][${s1} X ${e}][${s1} C ${e}][${s1} V ${e}][${s1} B ${e}][${s2} N ${e}][${s2} M ${e}][${s4} < ${e}][${s4} > ${e}][${s4} ? ${e}][${s3}SHIFT${e}][${s2}Fn${e}]
        [${s1}Fn ${e}][${s1} ALT ${e}][${s1}           SPACE           ${e}][${s2} ALT ${e}][${s2}WIN${e}]

-----------------------------------------------------------------------------
 [${s1}ESC${e}][${s1} F1${e}][${s4} F2${e}][${s1} F3${e}][${s4} F4${e}][${s1} F5${e}][${s2} F6${e}][${s2} F7${e}][${s2} F8${e}][${s2} F9${e}][${s4}F10${e}][${s2}F11${e}][${s2}F12${e}][${s4}INS${e}][${s4}DEL${e}]
 [${s1} TAB ${e}][${s1} q ${e}][${s1} w ${e}][${s1} e ${e}][${s1} r ${e}][${s1} t ${e}][${s2} y ${e}][${s2} u ${e}][${s4}PSc${e}][${s2}ScL${e}][${s2}Pus${e}][${s4} ￪ ${e}][${s2} [ ${e}][${s2}   BS ${e}]
 [${s1} CTRL  ${e}][${s1} a ${e}][${s1} s ${e}][${s1} d ${e}][${s5} f ${e}][${s1} g ${e}][${s2} * ${e}][${s6} j ${e}][${s4}Hom${e}][${s4}PgU${e}][${s4} ￩ ${e}][${s4} ￫ ${e}][${s2}   ENTER ${e}]
 [${s1} SHIFT  ${e}][${s1} z ${e}][${s1} x ${e}][${s1} c ${e}][${s1} v ${e}][${s1} b ${e}][${s2} + ${e}][${s2} \\ ${e}][${s4}End${e}][${s4}PgD${e}][${s4} ￬ ${e}][${s2}SHIFT${e}][${s3}Fn${e}]
        [${s3}Fn ${e}][${s1} ALT ${e}][${s1}           SPACE           ${e}][${s2} ALT ${e}][${s2}WIN${e}]

-----------------------------------------------------------------------------
 [${s1}ESC${e}][${s1} F1${e}][${s1} F2${e}][${s1} F3${e}][${s1} F4${e}][${s1} F5${e}][${s2} F6${e}][${s2} F7${e}][${s2} F8${e}][${s2} F9${e}][${s2}F10${e}][${s2}F11${e}][${s2}F12${e}][${s2}INS${e}][${s2}DEL${e}]
 [${s1} TAB ${e}][${s1} q ${e}][${s1} w ${e}][${s1} e ${e}][${s1} r ${e}][${s1} t ${e}][${s2} y ${e}][${s2} u ${e}][${s2}PSc${e}][${s2}ScL${e}][${s2}Pus${e}][${s2} ￪ ${e}][${s2} [ ${e}][${s2}   BS ${e}]
 [${s1} CTRL  ${e}][${s1} a ${e}][${s1} s ${e}][${s1} d ${e}][${s5} f ${e}][${s1} g ${e}][${s2} * ${e}][${s6} j ${e}][${s2}Hom${e}][${s2}PgU${e}][${s2} ￩ ${e}][${s2} ￫ ${e}][${s2}   ENTER ${e}]
 [${s3} SHIFT  ${e}][${s1} z ${e}][${s1} x ${e}][${s1} c ${e}][${s1} v ${e}][${s1} b ${e}][${s2} + ${e}][${s0} _ ${e}][${s2}End${e}][${s2}PgD${e}][${s2} ￬ ${e}][${s3}SHIFT${e}][${s3}Fn${e}]
        [${s3}Fn ${e}][${s1} ALT ${e}][${s1}           SPACE           ${e}][${s2} ALT ${e}][${s2}WIN${e}]

" | less -R

